{
    "editor.autoClosingBrackets": "always",
    "editor.bracketPairColorization.enabled": false,
    "editor.fastScrollSensitivity": 15,
    "editor.definitionLinkOpensInPeek": true,
    "editor.autoClosingQuotes": "always",
    "editor.smoothScrolling": true,
    "editor.cursorBlinking": "expand",
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.codeActionWidget.includeNearbyQuickFixes": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 500,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "workbench.commandPalette.preserveInput": true,
    "workbench.list.smoothScrolling": true,
    "workbench.statusBar.visible": false,
    "diffEditor.ignoreTrimWhitespace": false,

    // Mouse setting
    "editor.cursorStyle": "line-thin",
    "editor.cursorSmoothCaretAnimation": "on",
    "terminal.integrated.cursorBlinking": true,

    // Completely setting
    "editor.tabCompletion": "on",
    "editor.suggest.preview": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggest.showIcons": true,
    "editor.inlineSuggest.enabled": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.wordBasedSuggestionsMode": "allDocuments",
    "editor.parameterHints.enabled": true,
    "editor.quickSuggestionsDelay": 5,
    "editor.quickSuggestions":
    {
        "other": "on",
        "comments": "on",
        "strings": "on"
    },

    // Terminal settings
    "terminal.integrated.defaultProfile.windows": "Git Bash",

    // Explorer setting
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,

    // Workbench color custom
    "workbench.colorCustomizations":
    {
        "editor.background": "#121212",
        "editor.findMatchBorder": "#1F6FEB",
        "editor.findMatchBackground": "#1F6FEB30",
        "editor.findMatchHighlightBackground": "#b3b3b330",
        "editor.selectionBackground": "#1F6FEB50",
        "editor.lineHighlightBackground": "#1e1e1e",
        "editor.wordHighlightBackground": "#1F6FEB30",
        "editor.selectionHighlightBackground": "#1F6FEB30",
        "editorBracketMatch.border": "#1E90FF50",
        "editorSuggestWidget.border": "#1b1b1b",
        "editorSuggestWidget.background": "#121212",
        "editorWidget.border": "#1F6FEB",
        "editorWidget.background": "#121212",
        "button.background": "#238636",
        "button.hoverBackground": "#2ea043",
        "menu.background": "#121212",
        "menu.selectionBorder": "#1b1b1b",
        "sideBar.background": "#121212",
        "sideBar.border": "#121212",
        "sideBarSectionHeader.background": "#121212",
        "titleBar.activeBackground": "#121212",
        "titleBar.border": "#121212",
        "titleBar.inactiveBackground": "#121212",
        "list.hoverForeground": "#FFF",
        "list.hoverBackground": "#1b1b1b",
        "list.inactiveSelectionBackground": "#1E90FF30",
        "list.inactiveSelectionForeground": "#ffffff",
        "peekViewEditorGutter.background": "#121212",
        "peekViewEditor.background": "#121212",
        "terminal.border": "#1b1b1b",
        "terminal.background": "#121212",
        "profileBadge.background":"#187a46",
        "profileBadge.foreground":"#FFF",
        "activityBar.background": "#121212",
        "notifications.background": "#121212",
        "panel.border": "#1b1b1b",
        "input.border": "#1F6FEB"
    },

    // Custom color
    "editor.tokenColorCustomizations":
    {
        "textMateRules": [
            {
                "scope": [
                    "string",
                    "punctuation.definition.string"
                ],
                "settings": {
                    "foreground": "#A2D2FB",
                }
            },
            {
                "scope": [
                    "comment"
                ],
                "settings": {
                    "foreground": "#8B949E",
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "storage.modifier",
                    "storage.type",
                    "keyword",
                    "keyword.control",
                    "keyword.operator",
                    "keyword.other.unit",
                    "constant.character.escape"
                ],
                "settings": {
                    "foreground": "#FA7970",
                    "fontStyle": "italic bold"
                }
            },
            {
                "scope": [
                    "support.type",
                    "entity.name.tag",
                    "support.class.component",
                    "support.type.property-name",
                    "support.type.vendored.property-name.css",
                ],
                "settings": {
                    "foreground": "#7CE38B",
                }
            },
            {
                "scope": [
                    "variable.language",
                    "constant.other",
                    "support.class",
                    "support.other.namespace",
                    "variable.other.constant",
                    "entity.name.namespace",
                    "punctuation.definition.variable",
                    "entity.other.attribute-name.class.css",
                    "entity.other.attribute-name.pseudo-class.css",
                    "entity.other.attribute-name.id.css",
                    "entity.name.tag.css"
                ],
                "settings": {
                    "foreground": "#77BDFB",
                }
            },
            {
                "scope": [
                    "variable",
                    "text.html",
                    "punctuation.definition.tag",
                    "punctuation.separator.inheritance",
                    "invalid.illegal.character-not-allowed-here.html",
                ],
                "settings": {
                    "foreground": "#E6EDF3",
                }
            },
            {
                "scope": [
                    "constant.numeric",
                    "constant.language",
                    "entity.name.type",
                    "entity.other.inherited-class",
                    "punctuation.definition.template-expression.begin",
                    "punctuation.definition.template-expression.end",
                    "support.constant.property-value.css",
                    "constant.other.color.rgb-value.hex.css"
                ],
                "settings": {
                    "foreground": "#FAA356",
                }
            },
            {
                "scope": [
                    "entity.other.attribute-name",
                    "keyword.control.loop.vue",
                    "keyword.control.conditional.vue",
                ],
                "settings": {
                    "foreground": "#CEA5FB",
                }
            }
        ]
    },

    // Gitlen settings
    "diffEditor.codeLens": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.currentLine.pullRequests.enabled": false,
    "gitlens.currentLine.scrollable": false,
    "gitlens.showWelcomeOnInstall": false,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "gitlens.virtualRepositories.enabled": false,
    "gitlens.advanced.repositorySearchDepth": 1,
    "gitlens.currentLine.format": "ðŸ™„ ${author}, ${agoOrDate}",

    // Not Synced settings
    "settingsSync.ignoredSettings": [
        "editor.fontSize",
        "editor.fontFamily",
        "editor.lineHeight",
        "editor.fontWeight",
        "editor.letterSpacing",
        "editor.codeLensFontFamily",
        "terminal.integrated.fontFamily",
        "terminal.integrated.fontSize",
        "terminal.integrated.letterSpacing",
        "terminal.integrated.fontWeight",
        "vscode_custom_css.imports"
    ],
    "editor.fontSize": 19,
    "editor.lineHeight": 3.25,
    "editor.letterSpacing": 1.95,
    "terminal.integrated.fontSize": 18,
    "terminal.integrated.letterSpacing": 1.95,
    "editor.fontWeight": "450",
    "terminal.integrated.fontWeight": "600",
    "editor.fontFamily": "Victor Mono",
    "editor.codeLensFontFamily": "Victor Mono",
    "terminal.integrated.fontFamily": "Victor Mono",
    "vscode_custom_css.imports": [
        "file:///Users/thep200/Study/editor-config/themes/style.css",
        "file:///Users/thep200/.vscode/extensions/brandonkirbyson.vscode-animations-2.0.1/dist/updateHandler.js"
    ],

    // PHP setting
    "php.completion.parameters": "parentheses",
    "php.inlayHints.parameters.enabled": true,
    "php.inlayHints.parameters.suppressNameMatchingValue": true,
    "php.inlayHints.types.return": true,
    "php.hover.fullname": true,
    "php.linkedEditing.variables": true,
    "php.inlayHints.types.variable": true,

    // Daily settings change
    "unocss.colorPreview": false,
    "unocss.underline": false,
    "workbench.iconTheme": "eq-material-theme-icons-light",
    "zenMode.restore": true,
    "editor.accessibilitySupport": "off",
    "window.zoomLevel": -1.15,
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "editor.minimap.renderCharacters": false,
    "zenMode.showTabs": "none",
    "workbench.startupEditor": "none",
    "inlineChat.showGutterIcon": "never",
    "git.openRepositoryInParentFolders": "never",
    "workbench.sideBar.location": "right",
    "workbench.colorTheme": "Default Dark+",
    "window.commandCenter": false,
    "workbench.layoutControl.enabled": false,
    "workbench.editor.editorActionsLocation": "hidden",
    "editor.minimap.enabled": false,
    "workbench.editor.showTabs": "none",
}
