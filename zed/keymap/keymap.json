[
    {
        "context": "Workspace",
        "bindings": {
            "cmd-shift-g": "git_panel::ToggleFocus"
        }
    },
    {
        "context": "Workspace",
        "bindings": {
            "ctrl-r": [
                "projects::OpenRecent",
                {
                    "create_new_window": false
                }
            ]
        }
    },
    {
        "context": "Editor",
        "bindings": {
            "cmd-i": "assistant::InlineAssist"
        }
    },
    {
        "context": "Editor",
        "bindings": {
            "ctrl-alt-right": "pane::SplitRight"
        }
    },
    {
        "context": "Editor",
        "bindings": {
            "ctrl-alt-left": "pane::SplitLeft"
        }
    },
    {
        "context": "Editor",
        "bindings": {
            "ctrl-alt-up": "pane::SplitUp"
        }
    },
    {
        "context": "Editor",
        "bindings": {
            "ctrl-alt-down": "pane::SplitDown"
        }
    }
]
